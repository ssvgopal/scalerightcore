# Database Configuration
DATABASE_URL="postgresql://username:password@localhost:5432/orchestrall_dev"
DB_HOST=localhost
DB_PORT=5432
DB_NAME=orchestrall_dev
DB_USERNAME=username
DB_PASSWORD=password

# Server Configuration
PORT=3000
HOST=0.0.0.0
NODE_ENV=development

# Security
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=24h

# Redis Configuration (for caching and sessions)
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# AI Configuration
OPENAI_API_KEY=your-openai-api-key
AGENT_RUNTIME=openai

# Feature Flags
FEATURE_FLAGS_PROVIDER=local
UNLEASH_URL=http://unleash:4242/api
UNLEASH_API_TOKEN=your-unleash-token

# Deployment
DEPLOYMENT_TYPE=development
DEPLOYMENT_REGION=us-east-1

# Monitoring
LOG_LEVEL=info
ENABLE_METRICS=true

# PatientFlow WhatsApp Configuration
PATIENTFLOW_ORGANIZATION_ID=
PATIENTFLOW_SESSION_TTL=900
PATIENTFLOW_DEDUPE_TTL=86400
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_WHATSAPP_FROM_NUMBER=
TWILIO_VALIDATE_SIGNATURE=true
TWILIO_STATUS_CALLBACK_URL=
