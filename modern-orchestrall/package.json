{
  "name": "orchestrall-platform",
  "version": "2.0.0",
  "description": "Orchestrall Platform - Complete AI Agent Orchestration System",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app-commercial.js",
    "dev": "nodemon src/app-commercial.js",
    "simple": "node src/app-simple.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:backup": "node scripts/backup.js",
    "db:restore": "node scripts/restore.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "type-check": "tsc --noEmit",
    "build": "tsc",
    "docker:build": "docker build -t orchestrall/platform .",
    "docker:run": "docker run -p 3000:3000 orchestrall/platform",
    "k8s:deploy": "kubectl apply -f k8s/deployment.yaml",
    "k8s:status": "kubectl get pods -n orchestrall",
    "monitoring:start": "docker-compose -f docker-compose.prod.yml up -d prometheus grafana",
    "backup:create": "bash scripts/backup.sh",
    "backup:restore": "bash scripts/restore.sh",
    "security:scan": "npm audit",
    "security:fix": "npm audit fix"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^7.2.4",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@fastify/compress": "^6.0.1",
    "@fastify/request-context": "^3.0.0",
    "@prisma/client": "^5.7.1",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "fastify": "^4.24.3",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "openai": "^4.20.1",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "prisma": "^5.7.1",
    "redis": "^4.6.10",
    "uuid": "^9.0.1",
    "prom-client": "^15.0.0",
    "aws-sdk": "^2.1490.0",
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.4",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ai",
    "agents",
    "automation",
    "orchestration",
    "microservices",
    "platform",
    "langgraph",
    "crewai",
    "autogen"
  ],
  "author": "Orchestrall Team",
  "license": "MIT",
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
